  (quote ((autocad (2000 . 15.0)
        (2000i . 15.1)
        (2002 . 15.2)
        (2004 . 16.0)
        (2005 . 16.1)
        (2006 . 16.2)
        (2007 . 17.0)
        (2008 . 17.1)
        (2009 . 17.2)
        (2010 . 18.0)
        (2011 . 18.1)
        (2012 . 18.2)
        (2013 . 19.0)
        (2014 . 19.1)
        (2015 . 20.0)
        (2016 . 20.1)
        (2017 . 21.0)
        (2018 . 22.0)
        (2019 . 23.0)
        (2020 . 23.1)
        (2021 . 24.0)
        (2022 . 24.1))
      (gstarcad)
      (zwcad))))
