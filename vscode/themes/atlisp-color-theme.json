{
	"name": "atlisp-light",
	"type": "light",
	"colors": {
		"editor.background": "#ffffff",
		"editor.foreground": "#111"
	},
	"tokenColors": [
		{
			"name": "source",
			"scope": "source",
			"settings": {
				"foreground": "#222"
			}
		},
		{
			"name": "Parentheses",
			"scope": "Parentheses",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#fc0404"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#888686"
				
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment.block",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#bd8d8d"
				
			}
		},
		{
			"name": "Comments: Preprocessor",
			"scope": "comment.block.preprocessor",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#888"
			}
		},
		{
			"name": "Comments: Documentation",
			"scope": [
				"comment.documentation",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#8ea385"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#c42424"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#295ee2"
			}
		},
		{
			"name": "Types",
			"scope": [
				"storage.type",
				"support.type"
			],
			"settings": {
				"foreground": "#854a8a"
			}
		},
		{
			"name": "Classes",
			"scope": [
				"entity.name.type",
				"entity.other.inherited-class",
				"support.class"
			],
			"settings": {
				"foreground": "#581181"
			}
		},
		{
			"name": "Exceptions",
			"scope": "entity.name.exception",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Sections",
			"scope": "entity.name.section",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Numbers, Characters",
			"scope": [
				"constant.numeric",
				"constant.character",
				"constant"
			],
			"settings": {
				"foreground": "#3abe3a"
			}
		},
		{
			"name": "Strings",
			"scope": "string",
			"settings": {
				"foreground": "#f109d2"
			}
		},
		{
			"name": "Functions",
			"scope": "function",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#6c1c80"
			}
		},
		{
			"name": "Method",
			"scope": "method",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#463eb3"
			}
		},
		{
			"name": "Propertys",
			"scope": "property",
			"settings": {
				"foreground": "#307acf"
			}
		},
		{
			"name": "variable",
			"scope": [
				"variable",
				"variable.readonly"
			],
			"settings": {
				"foreground": "#3ff0e0"
			}
		},
		{
			"name": "symbols",
			"scope": [
				"symbol.autolisp"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#0ca094"
			}
		}
	],
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"variable.readonly:java": "#ff0011",
		"function": "#ff00bb",
		"property": "#ee00ff",
		"method": "#00ff00",
		"keyword": "#00aaee",
		"variable": "#3ff0e0"
	  }
}